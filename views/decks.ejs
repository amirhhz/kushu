<% for(var i = 0; i < decks.length; i++) { %>

<div id="listDeckContainer">
	<div id="listDeckImage">
		<img src="images/paris.jpg" alt="No Image">
	</div>
	<div id="listDeckInfo">
		<h2><%=decks[i].deck_name %></h2>
		<p><%=decks[i].summary %></p>
		<h3>Facts: <b>236</b></h3>
		<div id="listDeckControls">
			<a href="/deck/<%=decks[i].DECK_ID %>">Play</a>
			<a href="#">Browse</a>
			<a href="#">Remove</a>
			<a href="#">More +</a>
		</div>
		<div id="mobListDeckControls">
			<a href="/deck/<%=decks[i].DECK_ID %>">Play</a>
			<a href="#">More +</a>
		</div>
	</div>
</div>

<% } %>
