<div id="deckBroswerTable" align="center">
	<table align="center" cellpadding="0" cellspacing="0">
	<tr>
		<td><h2>Question</h2></br></td>
		<td><h2>Answer</h2></br></td>
		<td><h2>Remove</h2></br></td>
	</tr>
	<% for(var i = 0; i < deck.length; i++) { %>
		<tr>
			<td><%= deck[i].q %></td>
			<td><%= deck[i].a %></td>
			<td><a href "#">Remove</a></td>
		</tr>
	<% } %>
	</table>
</div>